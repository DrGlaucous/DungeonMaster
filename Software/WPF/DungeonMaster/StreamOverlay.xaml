<Window x:Class="DungeonMaster.StreamOverlay"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DungeonMaster"
        mc:Ignorable="d"
        Title="StreamOverlay" Height="128" Width="1024" Closed="Window_Closed">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/StreamOverlay2.png"/>
        </Grid.Background>
        <MediaElement x:Name="TestMediaElement" HorizontalAlignment="Left" Height="108" Margin="10,71,0,0" VerticalAlignment="Top" Width="247"/>
        <Button x:Name="Bttn" Content="Button" HorizontalAlignment="Left" Margin="10,210,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.777,-7.592" Click="Bttn_Click"/>

    </Grid>
</Window>
